<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.mobile.donut">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/donutdonut"
        android:label="@string/app_name"
        android:roundIcon="@drawable/icon_donut"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".AuthActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".JoinActivity" />
        <activity android:name=".MainActivity" />
        <activity android:name=".ListActivity" />
        <activity android:name=".LinkActivity" />
        <activity android:name=".GraphActivity" />
        <activity android:name=".SettingActivity" />

        <meta-data
            android:name="com.kakao.sdk.AppKey"
            android:value="@string/kakao_app_key" />

        <activity android:name=".AlarmActivity" />
        <activity android:name=".IntroActivity" />
        <activity android:name=".SetActivity" />
        <activity android:name=".SavingActivity" />
        <activity android:name=".RequestActivity" />
        <activity android:name=".FinishActivity"></activity>
    </application>

</manifest>